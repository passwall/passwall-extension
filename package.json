{
  "name": "passwall-extension",
  "version": "4.13.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build && node scripts/fix-manifest-mv3.js",
    "build:dev": "vite build --mode development && node scripts/fix-manifest-mv3.js",
    "package": "yarn build && cd dist && zip -r ../passwall-extension-v$(node -p \"require('../package.json').version\").zip . -x '*.DS_Store' && cd ..",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .eslintignore",
    "test": "vitest --passWithNoTests",
    "test:build": "yarn build && vitest run --passWithNoTests"
  },
  "dependencies": {
    "@kyvg/vue3-notification": "^3.1.3",
    "@otplib/preset-browser": "^12.0.1",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.5",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "floating-vue": "^5.2.2",
    "localforage": "^1.10.0",
    "pinia": "^2.1.7",
    "qr-scanner": "^1.4.2",
    "tldts": "^7.0.19",
    "vee-validate": "^4.12.5",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vue": "^3.4.15",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.2.5",
    "webextension-polyfill": "^0.11.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.23",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.4",
    "@vueuse/integrations": "^10.7.2",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^24.0.0",
    "sass": "^1.70.0",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.12",
    "vite-plugin-vue-devtools": "^7.0.13",
    "vitest": "^1.2.1",
    "webextension-polyfill-ts": "^0.26.0"
  }
}